{
    "manifest_version": 3,
    "name": "Smart Mic Controller",
    "version": "1.0",
    "description": "Mutes background voices, unmutes only your voice.",
    "permissions": ["scripting", "tabs", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["http://localhost:8765/"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  